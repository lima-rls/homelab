# Exemplo de variáveis sensíveis para o playbook proxmox_token.yml
# Copie para proxmox_access.yml e proteja com ansible-vault encrypt.
# Para ver ou editar depois, use: ansible-vault view vault/proxmox_access.yml ou ansible-vault edit vault/proxmox_access.yml
ansible_ssh_pass: "SENHA_DO_ROOT"

proxmox_user: "terraform"
proxmox_realm: "pve"
proxmox_password: "SenhaParaNovoUsuario"
proxmox_token_name: "automation"
# Secret retornado pelo comando `pveum user token add ...`
proxmox_token_secret: "INSIRA_AQUI_O_SECRET_GERADO"
proxmox_role: "PVEAdmin"
